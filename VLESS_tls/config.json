{
    "log": {
        "access": "/var/log/xray/access.log",
        "error": "/var/log/xray/error.log",
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": 10086,
            "listen": "127.0.0.1",
            "tag": "VLESS-xhttp-in",
            "protocol": "VLESS",
            "settings": {
              "clients": [
                {
                  "id": "UUID",
                  "level": 0,
                  "email": "me@hey.run"
                }
              ],
              "decryption": "none"
            },
            "streamSettings": {
              "network": "xhttp",
              "security": "none",
              "xhttpSettings": {
                "path": "/ray/"
              }
            }
          }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "tag": "blocked"
        }
    ],
    "dns": {
        "servers": [
            "8.8.8.8",
            "8.8.4.4",
            "1.1.1.1",
            "1.0.0.1",
            "localhost",
            "https+local://dns.google/dns-query",
            "https+local://1.1.1.1/dns-query"
        ]
    },
    "routing": {
        "domainStrategy": "AsIs",
        "rules": [
            {
                "type": "field",
                "inboundTag": [
                    "VLESS-xhttp-in"
                ],
                "outboundTag": "direct"
            }
        ]
    }
}